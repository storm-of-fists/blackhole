            name-[ function-fields ]

            [ field usable default-[ test ] ]
        ]

        [ Ono-Code-[ from-string ]
            [ template
                '
                [ function { [ spread function-fields ] } ] [ # write the function definition. ]
                [ { name } ] [ # Call the function. ]
                '
            ]
        ]
    ]
]

