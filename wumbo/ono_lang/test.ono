[ library Name-Tests
    usable-[ test ]

    [ function namespace-name-is-capitalized
        usable-[ test ]
    ]
]

[ function only-namespaces-are-capitalized
    test-[ true ]
    [ field code Ono-Code ]
    [
        [ render-template
            [
            [ all code]
            [ iterate
                semantics-[ move ]
            ]
            [ all code-[[ type ]] namespace [
                [ iter [ split namespace-name [ - ] ] segment [
                    [ is-capitalized [ index segment 0 ] ]
                ] ]
            ] ]
        ] ]
    ]
]


