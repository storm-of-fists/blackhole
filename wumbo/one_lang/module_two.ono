(# another define function, this time for modules #)
(module_define my_other_module (
    (# can reference a module by full namespace #)
    (fun_module StructOne constructor_one
        field_one 3121.21
    )

    (# can also import things from a module #)
    (import fun_module StructOne)
    (# try not to use symbols, use keywords #)
    (# to import all you can do (import fun_module ALL) #)

    (bind cool_variable StructOne (StructOne constructor_one
        field_one 213.1
    ))
))